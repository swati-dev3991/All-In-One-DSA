"><span>&nbsp;&nbsp;<span class="keyword">if</span><span>&nbsp;(res&nbsp;==&nbsp;-1)&nbsp;&nbsp;</span></span></li><li class="alt"><span>&nbsp;&nbsp;Console.WriteLine(<span class="string">"Element&nbsp;is&nbsp;not&nbsp;present&nbsp;in&nbsp;the&nbsp;array"</span><span>);&nbsp;&nbsp;</span></span></li><li class=""><span>&nbsp;&nbsp;<span class="keyword">else</span><span>&nbsp;&nbsp;</span></span></li><li class="alt"><span>&nbsp;&nbsp;Console.Write(<span class="string">"Element&nbsp;is&nbsp;present&nbsp;at&nbsp;"</span><span>&nbsp;+&nbsp;res&nbsp;+</span><span class="string">"&nbsp;position&nbsp;of&nbsp;array"</span><span>);&nbsp;&nbsp;</span></span></li><li class=""><span>}&nbsp;&nbsp;</span></li><li class="alt"><span>}&nbsp;&nbsp;</span></li></ol></div><textarea name="code" class="Cpp" style="display: none;">using System;
class LinearSearch {
static int linearSearch(int[] a, int n, int val) {
  // Going through array sequencially
  for (int i = 0; i &lt; n; i++)
    {
        if (a[i] == val)
        return i+1;
    }
  return -1;
}
static void Main() {
  int[] a = {56, 30, 20, 41, 67, 31, 22, 14, 52}; // given array
